{
  "name": "mask-xml",
  "version": "1.0.3",
  "description": "Redacts values in xml files.",
  "keywords": [
    "deep",
    "mask",
    "mask-xml",
    "redact"
  ],
  "homepage": "https://github.com/nunofgs/mask-xml",
  "bugs": {
    "url": "https://github.com/nunofgs/mask-xml/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/nunofgs/mask-xml.git"
  },
  "scripts": {
    "changelog": "github-changelog-generator --future-release=$npm_package_version --owner=nunofgs --repo=mask-xml > CHANGELOG.md",
    "lint": "eslint .",
    "test": "NODE_ENV=test mocha --require should test",
    "version": "npm run changelog --future-release=$npm_package_version && git add -A CHANGELOG.md"
  },
  "devDependencies": {
    "@uphold/github-changelog-generator": "^0.4.1",
    "eslint": "^6.2.2",
    "eslint-config-uphold": "^1.0.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "mocha": "^3.1.2",
    "should": "^11.1.1"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint --fix",
      "git add"
    ]
  }
}
