{
  "name": "mask-xml",
  "version": "0.0.2",
  "description": "Redacts values in xml files.",
  "keywords": [
    "deep",
    "mask",
    "mask-xml",
    "redact"
  ],
  "homepage": "https://github.com/seegno/mask-xml",
  "bugs": {
    "url": "https://github.com/seegno/mask-xml/issues"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/seegno/mask-xml.git"
  },
  "scripts": {
    "changelog": "github-changelog-generator --future-release=$npm_package_version --owner=seegno --repo=mask-xml > CHANGELOG.md",
    "lint": "eslint src test",
    "test": "NODE_ENV=test mocha --require should test",
    "transpile": "rm -rf dist/* && babel src --out-dir dist/src",
    "version": "npm run changelog --future-release=$npm_package_version && npm run transpile && git add -A CHANGELOG.md dist"
  },
  "devDependencies": {
    "@uphold/github-changelog-generator": "^0.4.1",
    "eslint": "^3.18.0",
    "eslint-config-seegno": "^9.0.0",
    "mocha": "^3.1.2",
    "pre-commit": "^1.0.10",
    "should": "^11.1.1"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "pre-commit": {
    "run": [
      "lint"
    ]
  }
}
